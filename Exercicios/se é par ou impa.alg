Algoritmo "semnome"

Var
   n:inteiro
procedimento ParOuImpar(V:inteiro)
inicio
   se(v%2 = 0)entao
      Escreval("O numero ", v," e par")
   senao
      escreval("O numero ", V," é impar")
   fimse
fimprocedimento

Inicio
   escreva("Digite um numero: ")
   leia (n)
   ParOuImpar(n)


Fimalgoritmo